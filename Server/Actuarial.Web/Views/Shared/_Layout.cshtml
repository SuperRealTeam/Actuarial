    <script src="~/js/Select2/select2.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            var date = new Date();
            var futureOneYear = new Date();
            futureOneYear.setDate(date.getDate() + 365);
            date.setDate(date.getDate() + 1);
            $('.futureDates').datepicker({
                minDate: date,
                format: 'mm/dd/yyyy',
                todayHighlight: true,
                autoclose: true,
                changeMonth: true,
                changeYear: true,

            })
            $('.futureOneYearDates').datepicker({
                maxDate: futureOneYear,
                minDate: date,
                format: 'mm/dd/yyyy',
                todayHighlight: true,
                autoclose: true,
                changeMonth: true,
                changeYear: true,

            })
            var date = new Date();
            date.setDate(date.getDate());
            $('.pastDates').datepicker({
                maxDate: date,
                format: 'mm/dd/yyyy',
                yearRange: "-100:+0",
                changeMonth: true,
                changeYear: true,
                todayHighlight: true,
                autoclose: true,
            })

            var futureOneYear = new Date();
            futureOneYear.setDate(date.getDate() + 365);
            $('.futureOneYearDates').datepicker({
                maxDate: futureOneYear,
                format: 'mm/dd/yyyy',
                todayHighlight: true,
                autoclose: true,
                changeMonth: true,
                changeYear: true,
            })

            $('.datepicker').datepicker({
                format: 'dd/mm/yyyy',
                todayHighlight: true,
                firstDay: 1,
                yearRange: "-100:+0",
                changeMonth: true,
                changeYear: true,
                changeMonth: true,
                changeYear: true,
                autoclose: true,
            })


            $(function () {
                var dateFormat = "dd/mm/yyyy",
                    from = $(".from-date")
                        .datepicker({
                            defaultDate: "+1w",
                            changeMonth: true,
                            numberOfMonths: 1
                        })
                        .on("change", function () {
                            to.datepicker("option", "minDate", getDate(this));
                        }),
                    to = $(".to-date").datepicker({
                        defaultDate: "+1w",
                        changeMonth: true,
                        numberOfMonths: 1
                    })
                        .on("change", function () {
                            from.datepicker("option", "maxDate", getDate(this));
                        });

                function getDate(element) {
                    var date;
                    try {
                        date = $.datepicker.parseDate(dateFormat, element.value);
                    } catch (error) {
                        date = null;
                    }

                    return date;
                }
            });

        });
    </script>
</body>

</html>