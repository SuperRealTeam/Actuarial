        <ul class="sidebar navbar-nav">
            <li class="nav-item active">
                <a class="nav-link" asp-action="Dashboard" asp-area="" asp-controller="Dashboard">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            
            <li class="nav-item">
                <a class="nav-link" asp-action="Employers" asp-area="" asp-controller="Employer">
